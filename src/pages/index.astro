---
import BaseLayout from '../layouts/BaseLayout.astro';
import Greeting from '../components/Greeting';
import FeedbackForm from '../components/FeedbackForm.astro';
import Interventions from '../components/Interventions.astro';
import '../styles/interventions.css';
const title = "Celine Wambst | Insectisation";
---
<BaseLayout title={title}>

<div class="test-interventions inside">
  <h2>Interventions</h2>
  <ul>
    <li>
      <a href="">
        <img src="/images/guepe.png">
        <h3>Nid de guêpes</h3>
      </a>
    </li>
    <li>
      <a href="">
        <img src="/images/frelon.png">
        <h3>Nid de frelons</h3>
      </a>
    </li>
    <li>
      <a href="">
        <img src="/images/rat.png">
        <h3>Rats & rongeurs</h3>
      </a>
    </li>
    <li>
      <a href="">
        <img src="/images/punaise-lit.png">
        <h3>Punaises de lit</h3>
      </a>
    </li>
    <li>
      <a href="">
        <img src="/images/cafard.png">
        <h3>Cafard</h3>
      </a>
    </li>
    <li>
      <a href="">
        <img src="/images/guepe.png">
        <h3>2 Cafards</h3>
      </a>
    </li>
  </ul>
</div>

<div class="inside flex professionnels">
  <h2 class="index_titre professionnel">Professionnels</h2>
  <ul>
    <li>Restaurants</li>
    <li>Boulangeries</li>
    <li>Hôtels</li>
    <li>Entreprises</li>
    <li>Usines</li>
  </ul>
  <img src="/images/hotel.jpeg" alt="">
</div>
<div class="inside">
  <h2 class="index_titre contact">Contact</h2>
</div>
<FeedbackForm />

</BaseLayout>